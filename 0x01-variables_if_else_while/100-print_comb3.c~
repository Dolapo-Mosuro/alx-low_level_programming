#include <stdio.h>

/**
 * main - prints all possible different combinations of two digits
 * Return: ALways 0 (Success)
 */
int main(void) {
// Loop through all digits from 0 to 9
for (int i = 0; i < 10; i++) {
// Loop through all remaining digits from i + 1 to 9
for (int j = i + 1; j < 10; j++) {
// Print the two digits separated by ", "
putchar(i + '0');
putchar(',');
putchar(' ');
putchar(j + '0');
putchar('\n');
}
}
return (0);
}
